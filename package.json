    "autoprefixer": "^10.4.21",
    "geist": "^1.5.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
